      <img src="<%=member.avatar%>" alt="foto_van_mijzelf">
      <h1>
        <%=member.name + " " + member.surname%>
      </h1>
    </section>  

    <article class="bio">
      <p class="bio_p"><%=member.bio.html%></p>
    </article> 

    <!-- FOUR CARDS -->
    <article>
      <img class="icon" src="/hot-chocolate.png" alt="icon_hot_chocolate" />
